<div class="container-lg">
  <%- partial('_partial/intro') %>

  <div class="blog-detail">
    <%- partial('_partial/navbar-secondary', {nav_selected: page.title}) %>

    <div class="body-secondary">

      <% if(page.title === 'tags'){ %>
      <%- list_tags() %>
      <% } %>
      <% if(page.title === 'categories'){ %>
      <%- list_categories() %>
      <% } %>
      <% if(page.title === 'links'){ %>
      <%- partial('_partial/links') %>
      <% } %>
    </div>
  </div>
</div>
